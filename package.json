{
  "name": "theqa-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "build:advanced": "node scripts/build.js",
    "build:cf": "node _build-info.js && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test:cache": "node test-cache-version.js",
    "health:cache": "node cache-health-check.js",
    "cache:clear": "node clear-cache.js",
    "prepare": "husky",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "type-check": "tsc --noEmit",
    "quality": "npm run lint && npm run format:check && npm run type-check && npm run test",
    "quality:fix": "npm run lint:fix && npm run format"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.8",
    "@cloudflare/workerd-darwin-arm64": "^1.20250910.0",
    "astro": "^5.13.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "vitest": "^3.2.4"
  }
}
